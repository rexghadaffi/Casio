@{
    Layout = null;
}

<html>
<title>Online Clearance</title>
<head>
    <link href="~/bootstrap-3/css/bootstrap.css" rel="stylesheet" />
    <link href="~/bootstrap-table/dist/bootstrap-table.css" rel="stylesheet" />
    <link href="~/metro-bootstrap/metro-bootstrap.css" rel="stylesheet" /> 
    <link href="~/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
   <script src="~/scripts/jquery-1.9.1.min.js"></script>
   
</head>
<body>
    @{ Html.RenderPartial("~/Views/Shared/_Navbar.cshtml"); }
     <div class="container">
         <div class="row">
              @RenderBody()
         </div>
     </div>
       
   

    <script src="~/bootstrap-3/js/bootstrap.js"></script>
    <script src="~/bootstrap-table/dist/bootstrap-table.js"></script>
    <script type="text/javascript">
        $(".table-record").bootstrapTable({
            cache: false,
            classes: 'table table-hover table-striped table-condensed', 
            pagination: true,
            striped: true,
            pagination: true,
            pageSize: 5,
            pageList: [10, 25, 50, 100, 200],
            search: true,
            showColumns: true,      
        });
    </script>
