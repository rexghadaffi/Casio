<div id="studentToolbar">
    <div class="form-inline">
        <div class="form-group">
            <div class="btn-group">
                <button id="btnShowUpload" type="button"
                        onclick="Refresh()"
                        class="btn btn-sm btn-primary">
                    <span class="fa fa-refresh"></span>
                </button>
            </div>
        </div>
        <div class="form-group">
            <div class="btn-group">
                <button id="btnShowUpload" type="button"
                        class="btn btn-sm btn-success"
                        data-toggle="modal"
                        data-target="#uploadStudent">
                    <span class="fa fa-upload"></span>
                </button>
            </div>
        </div>
        <div class="form-group">
            <div class="input-group">
                <input type="text"
                       placeholder="Search Student"
                       id="txtSearchStudent"
                       data-toggle="tooltip"
                       data-placement="bottom"
                       title="find student"
                       class="form-control input-sm" onemptied="Refresh()" />
            </div>
            <div class="btn-group">
                <button id="btnSearchWeek" type="submit" class="btn btn-sm btn-default"
                        style="margin-left:-5px"
                        onclick="SearchStudent()">
                    <span class="fa fa-search"></span>
                </button>
            </div>
        </div>
    </div>
</div>
<table id="tblstudent" class="table-records">
    <thead>
    <th data-field="StudentNumber">Student #</th>
    <th data-field="Lastname">Lastname</th>
    <th data-field="Firstname">Firstname</th>
    <th data-field="Middlename">Middlename</th>
    <th data-field="Program">Course</th>
    <th data-field="Year">Year Level</th>
    <th data-field="Term">Term</th>
    <th data-field="SchoolYear">A.Y</th>
    </thead>
</table>


